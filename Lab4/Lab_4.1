public class Main {
    public static void main(String[] args) {
        int[] evenNumbers = new int[50];
        int[] oddNumbers = new int[50];
// Заповнення масиву парними числами
        for (int i = 0, num = 0; i < 50; i++, num += 2) {
            evenNumbers[i] = num;
        }
// Заповнення масиву непарними числами
        for (int i = 0, num = 1; i < 50; i++, num += 2) {
            oddNumbers[i] = num;
        }
// Виведення масивів на консоль для перевірки
        System.out.println("Парні числа:");
        for (int num : evenNumbers) {
            System.out.print(num + " ");
        }
        System.out.println("\n\nНепарні числа:");
        for (int num : oddNumbers) {
            System.out.print(num + " ");
        }
    }
}


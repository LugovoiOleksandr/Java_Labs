public class Main {
    public static void main(String[] args) {
// Задайте масив чисел
        double[] numbers = {1.5, 2.7, 3.1, 4.6, 5.0}; // Введіть вашічисла тут
// Обчислити суму елементів масиву
        double sum = 0;
        for (int i = 0; i < numbers.length; i++) {
            sum += numbers[i];
        }
// Вивести суму
        System.out.println("Сума елементів масиву: " + sum);
    }
}


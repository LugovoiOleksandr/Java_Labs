import java.util.Scanner;
 class QuadraticEquationSolver {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Введіть коефіцієнт a: ");
        double a = input.nextDouble();

        System.out.print("Введіть коефіцієнт b: ");
        double b = input.nextDouble();
        System.out.print("Введіть коефіцієнт c: ");
        double c = input.nextDouble();
        double discriminant = b * b - 4 * a * c;
        if (discriminant < 0) {
            System.out.println("Рівняння не має дійсних коренів.");
        } else if (discriminant == 0) {
            double root = -b / (2 * a);
            System.out.println("Рівняння має один дійсний корінь: " +
                    root);
        } else {
            double root1 = (-b + Math.sqrt(discriminant)) / (2 * a);
            double root2 = (-b - Math.sqrt(discriminant)) / (2 * a);
            System.out.println("Рівняння має два дійсних корені: " +
                    root1 + " і " + root2);
        }
    }
}

